    <managed-bean>
        <managed-bean-name>exercicioMB</managed-bean-name>
        <managed-bean-class>br.cefetmg.inf.controller.mb.ExercicioMB</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>instrutorMB</managed-bean-name>
        <managed-bean-class>br.cefetmg.inf.controller.mb.InstrutorMB</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
